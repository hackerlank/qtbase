
dll {
#    QMAKE_LFLAGS_SHLIB += $$QMAKE_LFLAGS_BSYMBOLIC_FUNC

    !isEmpty(QMAKE_DYNAMIC_LIST_FILE):QMAKE_LFLAGS_SHLIB += $$QMAKE_LFLAGS_DYNAMIC_LIST$$QMAKE_DYNAMIC_LIST_FILE
    else:QMAKE_LFLAGS_SHLIB += $$QMAKE_LFLAGS_DYNAMIC_LIST$$PWD/empty.dynlist

    # plugins never have any dynamic symbols
    QMAKE_LFLAGS_PLUGIN += $$QMAKE_LFLAGS_DYNAMIC_LIST$$PWD/empty.dynlist
}
